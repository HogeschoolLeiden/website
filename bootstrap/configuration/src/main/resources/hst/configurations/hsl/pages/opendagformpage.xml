          <sv:value>omnikassavalidationpage/leftBottom</sv:value>
        </sv:property>
      </sv:node>
    </sv:node>
    <sv:node sv:name="content">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
        <sv:value>hst:component</sv:value>
      </sv:property>
      <sv:property sv:name="hst:componentclassname" sv:type="String">
        <sv:value>com.onehippo.cms7.eforms.hst.components.FormStoringEformComponent</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parameternames" sv:type="String" sv:multiple="true">
        <sv:value>eforms-body-html</sv:value>
        <sv:value>behaviors</sv:value>
        <sv:value>eforms-mailsession</sv:value>
        <sv:value>eforms-from-name</sv:value>
        <sv:value>eforms-from-email</sv:value>
        <sv:value>eforms-use-freemarker</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parametervalues" sv:type="String" sv:multiple="true">
        <sv:value>&lt;html xmlns="http://www.w3.org/1999/xhtml"&gt;&#13;
  &lt;head&gt;&#13;
    &lt;meta http-equiv="content-type" content="text/html;charset=UTF-8"/&gt;&#13;
    &lt;#if form.getTitle()??&gt;&#13;
      &lt;title&gt;${form.getTitle()}&lt;/title&gt;&#13;
    &lt;#elseif form.getDisplayName()??&gt;&#13;
      &lt;title&gt;${form.getDisplayName()}&lt;/title&gt;&#13;
    &lt;/#if&gt;&#13;
  &lt;/head&gt;&#13;
  &lt;body&gt;&#13;
    &lt;#if form.getTitle()??&gt;&#13;
      &lt;h1&gt;${form.getTitle()}&lt;/h1&gt;&#13;
    &lt;#elseif form.getDisplayName()??&gt;&#13;
      &lt;h1&gt;${form.getDisplayName()}&lt;/h1&gt;&#13;
    &lt;/#if&gt;&#13;
    &lt;#if paragraph??&gt;&#13;
      &lt;p&gt;${paragraph}&lt;/p&gt;&#13;
    &lt;/#if&gt;&#13;
    &lt;#if QR??&gt;&#13;
      &lt;img src="${QR}"/&gt;&#13;
    &lt;/#if&gt;&#13;
    &lt;#if includeFieldData&gt;&#13;
        &lt;table&gt;&#13;
          &lt;#list form.getFields() as field&gt;&#13;
            &lt;#if field.multiple&gt;&#13;
              &lt;#assign multiValues = field.multiValues&gt;&#13;
              &lt;#if multiValues??&gt;&#13;
                &lt;#list multiValues?keys as key&gt;&#13;
                  &lt;#assign collection = multiValues[key]&gt;&#13;
                  &lt;#list collection as itemValue&gt;&#13;
                    &lt;#if itemValue??&gt;&#13;
                      &lt;tr&gt;&#13;
                        &lt;th align="left"&gt;${key}&lt;/th&gt;&#13;
                        &lt;td align="left"&gt;${itemValue}&lt;/td&gt;&#13;
                      &lt;/tr&gt;&#13;
                    &lt;/#if&gt;&#13;
                  &lt;/#list&gt;&#13;
                &lt;/#list&gt;&#13;
              &lt;/#if&gt;&#13;
              &lt;#else&gt;&#13;
                &lt;#if field.value?has_content&gt;&#13;
                  &lt;tr&gt;&#13;
                    &lt;th align="left"&gt;${field.getLabel()}&lt;/th&gt;&#13;
                    &lt;td align="left"&gt;${field.getValue()}&lt;/td&gt;&#13;
                  &lt;/tr&gt;&#13;
                &lt;/#if&gt;&#13;
            &lt;/#if&gt;&#13;
          &lt;/#list&gt;&#13;
        &lt;/table&gt;&#13;
    &lt;/#if&gt;&#13;
  &lt;/body&gt;&#13;
&lt;/html&gt;</sv:value>
        <sv:value>nl.hsleiden.eforms.UUIDBehaivor,nl.hsleiden.eforms.QRMailFormDataBehavior</sv:value>
        <sv:value>mail/Session</sv:value>
        <sv:value>Webmaster</sv:value>
        <sv:value>noreply@hsleiden.nl</sv:value>
        <sv:value>true</sv:value>
      </sv:property>
      <sv:property sv:name="hst:resourcetemplate" sv:type="String">
        <sv:value>catalog.formvalidation</sv:value>
      </sv:property>
      <sv:property sv:name="hst:template" sv:type="String">
        <sv:value>catalog.formtemplate</sv:value>
      </sv:property>
      <sv:node sv:name="contentTop">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>hst:containercomponentreference</sv:value>
        </sv:property>
        <sv:property sv:name="hst:referencecomponent" sv:type="String">
          <sv:value>omnikassavalidationpage/contentTop</sv:value>
        </sv:property>
      </sv:node>
      <sv:node sv:name="rightTop">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>hst:containercomponentreference</sv:value>
        </sv:property>
        <sv:property sv:name="hst:referencecomponent" sv:type="String">
          <sv:value>omnikassavalidationpage/rightTop</sv:value>
        </sv:property>
      </sv:node>
      <sv:node sv:name="rightBottom">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>hst:containercomponentreference</sv:value>
        </sv:property>
        <sv:property sv:name="hst:referencecomponent" sv:type="String">
          <sv:value>omnikassavalidationpage/rightBottom</sv:value>
        </sv:property>
      </sv:node>
      <sv:node sv:name="contentBottom">
        <sv:property sv:name="jcr:primaryType" sv:type="Name">
          <sv:value>hst:containercomponentreference</sv:value>
        </sv:property>
        <sv:property sv:name="hst:referencecomponent" sv:type="String">
          <sv:value>omnikassavalidationpage/contentBottom</sv:value>
        </sv:property>
      </sv:node>
    </sv:node>
    <sv:node sv:name="bottom-container">
      <sv:property sv:name="jcr:primaryType" sv:type="Name">
        <sv:value>hst:containercomponentreference</sv:value>
      </sv:property>
      <sv:property sv:name="hst:referencecomponent" sv:type="String">
        <sv:value>omnikassavalidationpage/bottom-container</sv:value>
      </sv:property>
    </sv:node>
  </sv:node>
</sv:node>
