      </sv:property>
      <sv:property sv:name="hst:label" sv:type="String">
        <sv:value>Formulieren</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parameternames" sv:type="String" sv:multiple="true">
        <sv:value>ef_done_redirect</sv:value>
        <sv:value>eforms-from-name</sv:value>
        <sv:value>eforms-from-email</sv:value>
        <sv:value>behaviors</sv:value>
        <sv:value>eforms-use-freemarker</sv:value>
        <sv:value>eforms-mailsession</sv:value>
        <sv:value>eforms-body-html</sv:value>
        <sv:value>tryToUseMixin</sv:value>
      </sv:property>
      <sv:property sv:name="hst:parametervalues" sv:type="String" sv:multiple="true">
        <sv:value>bedank/bedank-pagina</sv:value>
        <sv:value>Hippo Site Info</sv:value>
        <sv:value>info@hsl.com</sv:value>
        <sv:value>com.onehippo.cms7.eforms.hst.behaviors.FormIntroBehavior,com.onehippo.cms7.eforms.hst.behaviors.StoreFormDataBehavior,nl.hsleiden.eforms.ConfirmationMailBehavior,com.onehippo.cms7.eforms.hst.behaviors.MailFormDataBehavior, com.onehippo.cms7.eforms.hst.behaviors.FormSubmissionCounterBehavior</sv:value>
        <sv:value>true</sv:value>
        <sv:value>mail/Session</sv:value>
        <sv:value>&lt;h1&gt;${form.getTitle()}&lt;/h1&gt;&#13;
&lt;#if paragraph??&gt;&#13;
	&lt;p&gt;${paragraph}&lt;/p&gt;&#13;
&lt;/#if&gt;&#13;
&lt;#if includeFieldData&gt;&#13;
	&lt;table&gt;&#13;
		&lt;#list form.getFields() as field&gt;&#13;
			&lt;#assign fieldType = field.type&gt;&#13;
			&lt;#assign multiple = field.isMultiple()&gt;&#13;
			&lt;#if multiple&gt;&#13;
				&lt;#if 'radiogroup' == fieldType&gt;&#13;
					&lt;#if field.getValue()?has_content&gt;&#13;
                        &lt;tr&gt;&#13;
							&lt;th align="left"&gt;${field.getLabelOrName()}&lt;/th&gt;&#13;
							&lt;td align="left"&gt;${field.getValue()}&lt;/td&gt;&#13;
                        &lt;/tr&gt;&#13;
					&lt;/#if&gt;&#13;
				&lt;#else&gt;&#13;
                    &lt;#if 'checkboxgroup' == fieldType&gt;&#13;
                        &lt;#assign multiValues = field.multiValues&gt;&#13;
						&lt;#list multiValues?keys as key&gt;&#13;
							&lt;#assign collection = multiValues[key]&gt;&#13;
							&lt;#list collection as itemValue&gt;&#13;
								&lt;#if itemValue??&gt;&#13;
									&lt;#if itemValue?has_content&gt;&#13;
										&lt;tr&gt;&#13;
											&lt;th align="left"&gt;${key}&lt;/th&gt;&#13;
											&lt;td align="left"&gt;${itemValue}&lt;/td&gt;&#13;
										&lt;/tr&gt;&#13;
									&lt;/#if&gt;&#13;
								&lt;/#if&gt;&#13;
							&lt;/#list&gt;&#13;
						&lt;/#list&gt;&#13;
                        &lt;#assign otherFieldName = field.getName()+"-other"&gt;&#13;
                        &lt;#assign otherField = formMap.getField(otherFieldName)&gt;&#13;
                        &lt;#if otherField.getValue()?has_content&gt;&#13;
							&lt;tr&gt;&#13;
	                            &lt;th align="left"&gt;${otherFieldName}&lt;/th&gt;&#13;
	                            &lt;td align="left"&gt;${otherField.getValue()}&lt;/td&gt;&#13;
                            &lt;/tr&gt;&#13;
                        &lt;/#if&gt;&#13;
                    &lt;#else&gt;&#13;
						&lt;#assign multiValues = field.multiValues&gt;&#13;
						&lt;#list multiValues?keys as key&gt;&#13;
							&lt;#assign collection = multiValues[key]&gt;&#13;
							&lt;#list collection as itemValue&gt;&#13;
								&lt;#if itemValue??&gt;&#13;
									&lt;#if itemValue?has_content&gt;&#13;
									&lt;tr&gt;&#13;
										&lt;th align="left"&gt;${key}&lt;/th&gt;&#13;
										&lt;td align="left"&gt;${itemValue}&lt;/td&gt;&#13;
									&lt;/tr&gt;&#13;
									&lt;/#if&gt;&#13;
								&lt;/#if&gt;&#13;
							&lt;/#list&gt;&#13;
						&lt;/#list&gt;&#13;
					&lt;/#if&gt;&#13;
				&lt;/#if&gt;&#13;
			&lt;#else&gt;&#13;
				&lt;#if field.getValue()?has_content&gt;&#13;
                    &lt;tr&gt;&#13;
						&lt;th align="left"&gt;${field.getLabelOrName()}&lt;/th&gt;&#13;
						&lt;#if 'datefield' == fieldType&gt;&#13;
							&lt;td align="left"&gt;${formMap.getField(field.getName()).getValue()}&lt;/td&gt; &#13;
						&lt;#else&gt;&#13;
							&lt;td align="left"&gt;${field.getValue()}&lt;/td&gt;&#13;
						&lt;/#if&gt;&#13;
                     &lt;/tr&gt;&#13;
				&lt;/#if&gt;&#13;
			&lt;/#if&gt;&#13;
		&lt;/#list&gt;&#13;
	&lt;/table&gt;&#13;
&lt;/#if&gt;</sv:value>
        <sv:value>true</sv:value>
