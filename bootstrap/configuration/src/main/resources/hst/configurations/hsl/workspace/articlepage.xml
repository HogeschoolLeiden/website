      <sv:property sv:name="hst:parametervalues" sv:type="String" sv:multiple="true">
        <sv:value>bedank/bedank-pagina</sv:value>
        <sv:value>Hippo Site Info</sv:value>
        <sv:value>info@hsl.com</sv:value>
        <sv:value>com.onehippo.cms7.eforms.hst.behaviors.FormIntroBehavior,com.onehippo.cms7.eforms.hst.behaviors.StoreFormDataBehavior,nl.hsleiden.eforms.ConfirmationMailBehavior,com.onehippo.cms7.eforms.hst.behaviors.MailFormDataBehavior</sv:value>
        <sv:value>true</sv:value>
        <sv:value>mail/Session</sv:value>
        <sv:value>&lt;h2&gt;Support form filled&lt;/h2&gt;&#13;
              &lt;table&gt;&#13;
                    &lt;#list form.getFields() as field&gt;&#13;
                      &lt;#if field.multiple&gt;&#13;
                        &lt;#assign multiValues = field.multiValues&gt;&#13;
                        &lt;#if multiValues??&gt;&#13;
                          &lt;#list multiValues?keys as key&gt;&#13;
                            &lt;#assign collection = multiValues[key]&gt;&#13;
                            &lt;#list collection as itemValue&gt;&#13;
                              &lt;#if itemValue??&gt;&#13;
                                &lt;tr&gt;&#13;
                                  &lt;th align="left"&gt;${key}&lt;/th&gt;&#13;
                                  &lt;td align="left"&gt;${itemValue}&lt;/td&gt;&#13;
                                &lt;/tr&gt;&#13;
                              &lt;/#if&gt;&#13;
                            &lt;/#list&gt;&#13;
                          &lt;/#list&gt;&#13;
                        &lt;/#if&gt;&#13;
                        &lt;#else&gt;&#13;
                          &lt;#if field.value?has_content&gt;&#13;
                            &lt;tr&gt;&#13;
                              &lt;th align="left"&gt;${field.getLabel()}&lt;/th&gt;&#13;
                              &lt;td align="left"&gt;${field.getValue()}&lt;/td&gt;&#13;
                            &lt;/tr&gt;&#13;
                          &lt;/#if&gt;&#13;
                      &lt;/#if&gt;&#13;
                    &lt;/#list&gt;&#13;
                  &lt;/table&gt;</sv:value>
        <sv:value>on</sv:value>
